import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        float sum=0;
        for(int i=0;i<n;i++)
        {
            sum +=sc.nextFloat();
        }
        float mean = sum/n;
        System.out.printf("Array Mean Value is %.2f " , mean);
        
        }
}
